<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AgendaDeContactoProgramacion3.ContactPage" Padding="10" Title="Contacto">
    <StackLayout>
        <Label Text="{Binding Nombre}" FontSize="20" />
        <Label Text="{Binding Telefono}" FontSize="20" />
        <Label Text="{Binding Correo}" FontSize="20" />
    </StackLayout>
</ContentPage>